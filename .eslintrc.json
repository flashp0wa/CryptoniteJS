{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "linebreak-style": 0,
        "camelcase":"off",
        "require-jsdoc":"off",
        "max-len":"off"
    }